[32m+[m[32mfrom collections import Counter[m
[32m+[m[32mimport sys[m
[32m+[m[32mimport time[m
[32m+[m
[32m+[m[32mfile_name=sys.argv[1][m
[32m+[m[32mwith open(file_name,'r') as file:[m
[32m+[m[32m    all_line=file.readlines()[m
[32m+[m[32m    all_number = [num.split(',')[0] for num in all_line][m
[32m+[m[32m    counter=dict(Counter(all_number))[m
[32m+[m[32m    sorted_counter = sorted(counter.items(), key=lambda d: d[1], reverse=True)[m
[32m+[m[32m    #sorted_counter ä¸ºlistç±»åž‹[m
[32m+[m[32mnew_file = time.strftime("%Y%m%d%H%M%S",time.localtime()) + '_' + file_name[m
[32m+[m[32mwith open(new_file,'w') as file:[m
[32m+[m[32m    for l in sorted_counter:[m
[32m+[m[32m        file.write(str(l[0]) + ','+ str(l[1]) + '\n')[m
